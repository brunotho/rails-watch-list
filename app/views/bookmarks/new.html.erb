<div class="container">
<h2>new bookmark form</h2>
  <%= simple_form_for [@list, @bookmark], html: { class: 'form-horizontal' },wrapper: :horizontal_form do |f| %>
    <%= f.input :comment %>
    <%= f.association :movie %>
    <%= f.submit "Submit review", class: "btn btn-primary" %>
  <% end %>
</div>
